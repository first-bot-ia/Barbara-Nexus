# Archivo de ejemplo para configurar variables de entorno para AventuraPe
# Copia este archivo a .env y personaliza las variables según tu entorno

# API key de Gemini AI para generar contenido
GEMINI_API_KEY=tu_api_key_aqui

# URL base para la API de AventuraPe
AVENTURAPE_API_URL=http://localhost:8080

# Configuración del entorno
ENV=development
DEBUG=True
LOG_LEVEL=INFO

# Puerto para el servidor Flask
PORT=5000 